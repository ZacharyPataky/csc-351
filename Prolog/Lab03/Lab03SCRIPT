% file: /mnt0/stud/patakyz/CSC351/Lab/Lab03/queens.pl

queens(A, B) :-
	range(1, A, C),
	my_permutation(C, B),
	safe(B),
	print_board(B, A).

print_board(A, B) :-
	nl,
	print_boardHelper(A, B).

print_boardHelper([A|B], C) :-
	writeBars(C),
	nl,
	writeChar(A, C, 0),
	write('|'),
	nl,
	print_boardHelper(B, C).
print_boardHelper([], A) :-
	writeBars(A).

writeChar(A, B, C) :-
	A > 1,
	C =:= 0,
	D is A - 1,
	E is B - 1,
	write('| '),
	writeChar(D, E, C).
writeChar(A, B, C) :-
	A =:= 1,
	C < 1,
	B > 0,
	D is B - 1,
	write('|Q'),
	writeCharHelper(D, 1).

writeCharHelper(A, B) :-
	B > 0,
	A > 0,
	C is A - 1,
	write('| '),
	writeCharHelper(C, B).
writeCharHelper(0, 1).

writeBars(A) :-
	write(--),
	A > 0,
	B is A - 1,
	writeBars(B).
writeBars(0).

my_permutation(A, [B|C]) :-
	myselect(B, A, D),
	my_permutation(D, C).
my_permutation([], []).

myselect(A, [A|B], B).
myselect(A, [B|C], [B|D]) :-
	myselect(A, C, D).

range(A, B, [A|C]) :-
	A < B,
	D is A + 1,
	range(D, B, C).
range(A, A, [A]).

safe([A|B]) :-
	\+ attacked(A, B),
	safe(B).
safe([]).

attacked(A, B) :-
	attackedHelper(A, 1, B).

attackedHelper(A, B, [bRow|bRows]) :-
	(   A =:= bRow + B
	;   A =:= bRow - B
	).
attackedHelper(A, B, [bRow|bRows]) :-
	C = B + 1,
	attackedHelper(A, C, bRows).

yes
| ?- queens(8, Y_ )(8, Y).

